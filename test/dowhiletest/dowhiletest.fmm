# At first, we try to calculate the factorial with a simple loop

let num=10
let fact=1
do
	let fact=fact*num
	let num=num-1
while num>0

print fact

# Then, we put together two loops, either for-next and do-loop

let matrix(100)=1
let ck=0

# Two for-next loops
# Creation of data table

for :i 0 10 1
	for :j 0 20 1
		let matrix(i+j*10)=i*j
		let ck=ck+matrix(i+j*10)
	next
next


# Two do-while loops
let i=0 j=0
let checksum=0
do
	let i=0
	do
		let checksum=checksum+matrix(i+j*10)
		#print i j checksum
		let i=i+1
	while i<10
	let j=j+1
while j<20

# One for-next and one do-while
let i=0 j=0
let checksum1=0
do
	for :i 0 10 1
		let checksum1=checksum1+matrix(i+j*10)
	next
	let j=j+1
while j<20

# One do-while and one for-next
let i=0 j=0
let checksum2=0
for :j 0 20 1
	let i=0
	do
		let checksum2=checksum2+matrix(i+j*10)
		let i=i+1
